$w.onReady(function () {
    $w('#button1').enable();
    $w('#button2').disable();
    if ($w('#textBox1').value === getUsername() && $w('#textBox2').value === getPassword()) {
        $w('#button2').enable();
        $w('#button1').disable();
        setLogin(true);
    }
    $w("#button1").onClick(()=>{
        
    }

});
